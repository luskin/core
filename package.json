{
  "name": "core",
  "private": true,
  "scripts": {
    "-----CLEAN-----": "-----CLEAN-----",
    "clean": "turbo clean && rm -rf node_modules .turbo && pnpm i",
    "-----DEPENDENCY MANAGEMENT-----": "-----DEPENDENCY MANAGEMENT-----",
    "add:web": "pnpm add --filter web",
    "add:dashboard": "pnpm add --filter dashboard",
    "add:docs": "pnpm add --filter docs",
    "add:backend": "pnpm add --filter backend",
    "add:ui": "pnpm add --filter @repo/ui",
    "add:utils": "pnpm add --filter @repo/utils",
    "remove:docs": "pnpm remove --filter docs",
    "remove:dashboard": "pnpm remove --filter dashboard",
    "-----BUILD-----": "-----BUILD-----",
    "build": "turbo build",
    "build:web": "turbo build --filter web",
    "build:docs": "turbo build --filter docs",
    "build:backend": "turbo build --filter backend",
    "build:dashboard": "turbo build --filter dashboard",
    "-----DEVELOPMENT-----": "-----DEVELOPMENT-----",
    "dev": "turbo dev",
    "dev:dashboard": "turbo dev --filter dashboard",
    "dev:docs": "turbo dev --filter docs",
    "-----START-----": "-----START-----",
    "start": "turbo start",
    "start:web": "turbo start --filter web",
    "start:docs": "turbo start --filter docs",
    "start:backend": "turbo start --filter backend",
    "----FORMAT-----": "----FORMAT-----",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "-----TEST-----": "-----TEST-----",
    "test:utils": "turbo test --filter @repo/utils"
  },
  "devDependencies": {
    "@configs/eslint": "workspace:*",
    "@configs/typescript": "workspace:*",
    "prettier": "^3.1.0",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=20.9.0"
  }
}
